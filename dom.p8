pico-8 cartridge // http://www.pico-8.com
version 14
__lua__

-- constants
max_hand_size = 4

-- vars
dbg1 = ''
dbg2 = ''
dbg3 = ''
dbg4 = ''

deck_idx = 1
deck = {}
hand = {}

selected_card_in_hand = 1

-- special callbacks
function _init()
  put_cards_in_deck()
  put_cards_in_hand()
end

function _update60()
  -- print next card
  if btnp(5) then
    deck_idx = nextidx(deck_idx, len(deck))
  end

  if btnp(0) then
    pressed_left()
  end

  if btnp(1) then
    pressed_right()
  end

  local cur_card = deck[deck_idx]

  if cur_card then
    dbg1 = cur_card.value
  end

  dbg2 = selected_card_in_hand
  dbg3 = len(hand)
end

function _draw()
  cls(1)
  draw_hand()
  debug(0, 0)
end

-- input functions ---------------------------------------------
function pressed_left()
  selected_card_in_hand = previdx(selected_card_in_hand, len(hand))
end

function pressed_right()
  selected_card_in_hand = nextidx(selected_card_in_hand, len(hand))
end

-- drawing functions ---------------------------------------------

-- note: 4 cards in hand max
function draw_hand()
  for i = 1,max_hand_size do
    local card = hand[i]
    draw_card(card, i)
  end
end

function draw_card(card, card_idx)
  local card_w = 16
  local card_h = 32
  local card_spacing = 12
  local card_spacing_vert = 4
  local x0 = card_spacing + (card_idx - 1) * (card_w + card_spacing)
  local y0 = 128 - (card_h + card_spacing_vert)

  -- also check for mode == selecting
  if selected_card_in_hand == card_idx then
    y0 -= 2
    spr(1, x0 + 4, y0 - 10)
  end

  rectfill(x0, y0, x0 + card_w, y0 + card_h, 6)
  print(card.value, x0 + 2, y0 + 2, 0)
end

-- utility functions ---------------------------------------------

-- returns the next index, looping over max
-- 1,3 -> 2
-- 2,3 -> 3
-- 3,3 -> 1
function nextidx(cur_idx, max)
  return cur_idx % max + 1
end

-- returns the prev index, looping over max
-- 1,3 -> 3
-- 2,3 -> 1
-- 3,3 -> 2
function previdx(cur_idx, max)
  local p = cur_idx - 1
  if (p == 0) then p = max end
  if (p > max) then p = max end

  return p
end

-- get length of metatable
function len(t)
  local c = 0
  for index, value in pairs(t) do
    c += 1
  end
  
  return c
end

function put_cards_in_deck()
  for i = 1,5 do
    local c = make_card()
    add(deck, c)
  end
end

function put_cards_in_hand()
  for i = 1,max_hand_size do
    local c = make_card()
    add(hand, c)
  end
end

function make_card()
  local c = {
    value = rndint(50)
  }
  return c
end

-- from 0 to x inclusive
function rndint(x)
  return flr(rnd(x + 1))
end

function debug(x,y)
  color(7)
  local line_dist = 10
  print(dbg1,x,y)
  print(dbg2,x,y+line_dist)
  print(dbg3,x,y+line_dist*2)
  print(dbg4,x,y+line_dist*3)
end

